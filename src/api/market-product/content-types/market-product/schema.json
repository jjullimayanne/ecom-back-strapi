      "type": "string"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "images": {
      "type": "json",
      "multiple": true
    },
    "unitOfMeasure": {
      "type": "string",
      "required": true
    },
    "stock": {
      "type": "integer",
      "required": true
    },
    "weight": {
      "type": "string"
    },
    "sku": {
      "type": "string",
      "required": true
    },
    "availability": {
      "type": "boolean",
      "default": true
